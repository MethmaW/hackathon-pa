{
	"name": "create-react-app-typescript-redux",
	"version": "2.0.0",
	"dependencies": {
		"@craco/craco": "^6.3.0",
		"@headlessui/react": "^1.4.1",
		"@heroicons/react": "^1.0.4",
		"localforage": "1.7.3",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-redux": "7.1.3",
		"react-router-dom": "5.1.2",
		"react-scripts": "4.0.3",
		"redux-devtools-extension": "2.13.8",
		"redux-logger": "3.0.6",
		"redux-persist": "6.0.0",
		"redux-thunk": "2.3.0",
		"typescript": "3.7.5"
	},
	"scripts": {
		"start": "craco start --verbose",
		"build": "craco build",
		"test": "craco test",
		"eject": "react-scripts eject",
		"format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|md|vue)\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.+(js|jsx|ts|tsx)": [
			"prettier --write",
			"git add"
		],
		"*.+(json|yml|yaml|md|vue)": [
			"prettier --write"
		],
		"*.+(css|scss)": [
			"prettier --write"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@types/jest": "24.0.25",
		"@types/node": "12.12.18",
		"@types/react": "16.9.19",
		"@types/react-dom": "16.9.5",
		"@types/react-redux": "7.1.7",
		"@types/react-router-dom": "5.1.3",
		"@types/redux-logger": "3.0.7",
		"@types/webpack-env": "1.15.1",
		"autoprefixer": "^9.8.6",
		"eslint-plugin-react": "7.17.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.6",
		"postcss": "^7.0.36",
		"prettier": "^2.0.5",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.14"
	}
}
